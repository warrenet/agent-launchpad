{
  "name": "agent-launchpad",
  "version": "1.0.0",
  "description": "AI Agent Launchpad - One-click deployment for AI agents with Gemini, auto-documentation and mobile-first testing UI",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "generate-docs": "node scripts/generate-docs.js",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel"
  },
  "dependencies": {
    "next": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "optionalDependencies": {
    "@anthropic-ai/sdk": "^0.39.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.2.0",
    "jest": "^29.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/warrenet/agent-launchpad.git"
  },
  "keywords": [
    "ai",
    "agent",
    "automation",
    "gemini",
    "google-ai",
    "claude",
    "openai",
    "llm",
    "github-actions",
    "vercel",
    "mobile-first",
    "pwa"
  ],
  "author": "Warren E. T.",
  "license": "MIT"
}
